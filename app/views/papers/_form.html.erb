<%= simple_form_for @paper do |paper| %>
    <div class="field">
        <div class="control">
            <%= paper.input :subject,placeholder: 'Enter Paper Subject', input_html: {class: "input"},label_html: {class: "label"},wrapper: false %>
        </div>
    </div>
    <div class="field">
        <%= paper.submit "Create Paper",class: "button is-primary" %>
    </div>
<% end %>